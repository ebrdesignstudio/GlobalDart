Props(nextProps){\n        if(this.props.value !== nextProps.value){\n            this.setState({ \n                value: nextProps.value,\n            })\n            this.props.setValue(nextProps.value);\n        }\n    }\n\n    render() {\n        return (\n            <div className=\"textarea__container caption\">\n                <textarea\n                    className = {this.props.className}\n                    name={this.props.name}\n                    disabled={!this.props.edit}\n                    placeholder={this.props.placeholder}\n                    onFocus={this.props.onFocus}\n                    onBlur={this.props.onBlur}\n                    onChange={this.changeValue}\n                    value={this.state.value}\n                />\n            </div>\n        );\n    }\n}\n\n\nTextArea.defaultProps = {\n    onBlur: _.noop,\n    onFocus: _.noop,\n    edit: true,\n};\n\nexport default {\n    formInput: withFormsy(TextArea),\n    input: CustomFormHOC(TextArea),\n};\n","/Users/lak/Desktop/GlobalDart/src/Web-bricks/Shared/Input/checkbox.js",[],"/Users/lak/Desktop/GlobalDart/src/Web-bricks/Shared/Input/textField.js",[],"/Users/lak/Desktop/GlobalDart/src/Web-bricks/Shared/Input/radio.js",[],"/Users/lak/Desktop/GlobalDart/src/Web-bricks/Shared/Input/CheckboxList.js",[],"/Users/lak/Desktop/GlobalDart/src/Web-bricks/Shared/files/excelToJSON.js",["157"],"import React, { Component } from 'react';\nimport _ from 'lodash'\nimport XLSX from 'xlsx';\nimport { withFormsy } from 'formsy-react';\nimport { Button } from '@material-ui/core';\nimport { CloudUpload } from '@material-ui/icons';\n \n\nconst SheetJSFT = [\n\t\"xlsx\", \"xlsb\", \"xlsm\", \"xls\", \"xml\", \"csv\", \"txt\", \"ods\", \"fods\", \"uos\", \"sylk\", \"dif\", \"dbf\", \"prn\", \"qpw\", \"123\", \"wb*\", \"wq*\", \"html\", \"htm\"\n].map(function(x) { return \".\" + x; }).join(\",\");\n\n/* generate an array of column objects */\nconst make_cols = refstr => {\n\tlet o = [], C = XLSX.utils